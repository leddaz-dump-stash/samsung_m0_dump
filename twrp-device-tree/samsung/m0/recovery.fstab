/boot         emmc     /dev/block/mmcblk0p5      flags=display=boot
/recovery     emmc     /dev/block/mmcblk0p6      flags=display=recovery
/system       ext4     /dev/block/mmcblk0p9      flags=display=system
/data         ext4     /dev/block/mmcblk0p12     flags=display=data
/cache        ext4     /dev/block/mmcblk0p8      flags=display=cache
/sdcard       vfat     /dev/block/mmcblk1p1      flags=display=sdcard
/efs          ext4     /dev/block/mmcblk0p3      flags=display=efs
/modem        emmc     /dev/block/mmcblk0p7      flags=display=modem
/preload      ext4     /dev/block/mmcblk0p10     flags=display=preload
